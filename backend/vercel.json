{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "JWT_SECRET": "@jwt_secret",
    "OPENAI_API_KEY": "@openai_api_key",
    "PINATA_API_KEY": "@pinata_api_key",
    "PINATA_SECRET_KEY": "@pinata_secret_key",
    "PINATA_JWT": "@pinata_jwt"
  }
}

